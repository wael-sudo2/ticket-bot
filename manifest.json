{
    "manifest_version": 3,
    "name": "Bot Ticket Notifications",
    "version": "1",
    "description": "A Chrome extension to scrape drops and notify",
    "permissions": ["activeTab", "scripting", "storage"],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icons/icon.png"
      }
    }
  }
  